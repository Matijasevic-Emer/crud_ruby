<div id="<%= dom_id subscription %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Type:</strong>
    <%= subscription.type %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= subscription.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= subscription.description %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Format:</strong>
    <%= subscription.format %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price:</strong>
    <%= subscription.price %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Start:</strong>
    <%= subscription.start %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Expiration:</strong>
    <%= subscription.expiration %>
  </p>

</div>
